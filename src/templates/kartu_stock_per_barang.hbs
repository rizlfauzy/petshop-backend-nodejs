<html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <style>
      * { font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif; /* font-family: Arial, Helvetica, sans-serif; */ /* font-family: 'Courier New', Courier, monospace; */ /* font-weight: bold; */ } @page { margin-top:
      25px; } .cf:before, .cf:after { content: " "; /* 1 */ display: table; /* 2 */ } .cf:after { clear: both; } /** * For IE 6/7 only * Include this rule to trigger hasLayout and contain floats. */ table { /* font-family: Verdana, Arial,
      Helvetica, sans-serif; */ border-collapse: collapse; width: 100%; } th { border: 1px solid #333; text-align: left; padding: 3px; } div { font-size: 12; /* border: 1px solid #408080; */ }
    </style>
    <title>PDF TEST</title>
  </head>

  <body>
    <h4 style="text-align: center;font-size: 12px;">{{title}}</h4>
    <div class="row cf">
      <div class="col cf" style="float: left; width: 50%; text-align: left;">
        <div style="font-size: 8px; width: 20%; float: left;">
          NAMA BARANG
        </div>
        <div style="font-size: 8px; width: 80%; float: right;">
          :
          {{barang.barcode}}
          -
          {{barang.nama}}
        </div>
      </div>
    </div>
    <div class="row cf">
      <div class="col cf" style="float: left; width: 50%; text-align: left;vertical-align: middle;">
        <div style="font-size: 8px; width: 20%; float: left;">
          PERIODE
        </div>
        <div style="font-size: 8px; width: 80%; float: right;">
          :
          {{tglawal}}
          <sup style="font-size: 10px;">s</sup>/<sub style="font-size: 10px;">d</sub>
          {{tglakhir}}
        </div>
      </div>
      <div class="col" style="float: right; width: 50%; text-align: right;">
        <div style="font-size: 8px; width: 45%; float: left;">
        </div>
        <div style="font-size: 8px; width: 55%; float: right;">
          Dicetak :
          {{waktu_cetak}}
        </div>
      </div>
    </div>

    <table class="border" style="margin-top: 10px; vertical-align: top;" width="100%">
      <thead>
        <tr>
          <th class="border" style="line-height: 1.5; text-align: center;font-size: 9px;">No</th>
          <th class="border" style="line-height: 1.5; text-align: center;font-size: 9px;">Keterangan</th>
          <th class="border" style="line-height: 1.5; text-align: center;font-size: 9px;">No Invoice</th>
          <th class="border" style="line-height: 1.5; text-align: center;font-size: 9px;">Tanggal</th>
          <th class="border" style="line-height: 1.5; text-align: center;font-size: 9px;">Stock</th>
        </tr>
        <tr>
          <th colspan="4" class="border" style="line-height: 1.5; text-align: right;font-size: 9px;padding-right: 6px;">Stock Awal : </th>
          <th class="border" style="line-height: 1.5; text-align: right;font-size: 9px;">{{stock_awal.qty_awal}} {{stock_awal.nama_satuan}}</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th colspan="5" class="border" style="line-height: 1.5; text-align: left;font-size: 9px;">Barang Masuk</th>
        </tr>
        {{#each data_details_masuk}}
          <tr>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.no}}</td>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.sub_keterangan}}</td>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.nomor}}</td>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.tanggal}}</td>
            <td class="border" style="line-height: 1.3; text-align: right;font-size: 8px;">{{this.qty_masuk}} {{this.nama_satuan}}</td>
          </tr>
        {{/each}}
        <tr>
          <th colspan="4" class="border" style="line-height: 1.5; text-align: right;font-size: 9px;padding-right: 6px;">Total Barang Masuk : </th>
          <th class="border" style="line-height: 1.5; text-align: right;font-size: 9px;">{{total_qty_masuk}} {{stock_awal.nama_satuan}}</th>
        </tr>
        <tr>
          <th colspan="5" class="border" style="line-height: 1.5; text-align: left;font-size: 9px;">Barang Keluar</th>
        </tr>
        {{#each data_details_keluar}}
          <tr>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.no}}</td>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.sub_keterangan}}</td>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.nomor}}</td>
            <td class="border" style="line-height: 1.3; text-align: left;font-size: 8px;">{{this.tanggal}}</td>
            <td class="border" style="line-height: 1.3; text-align: right;font-size: 8px;">{{this.qty_keluar}} {{this.nama_satuan}}</td>
          </tr>
        {{/each}}
        <tr>
          <th colspan="4" class="border" style="line-height: 1.5; text-align: right;font-size: 9px;padding-right: 6px;">Total Barang Keluar : </th>
          <th class="border" style="line-height: 1.5; text-align: right;font-size: 9px;">{{total_qty_keluar}} {{stock_awal.nama_satuan}}</th>
        </tr>
        <tr>
          <th colspan="4" class="border" style="line-height: 1.5; text-align: right;font-size: 9px;padding-right: 6px;">Stock Akhir : </th>
          <th class="border" style="line-height: 1.5; text-align: right;font-size: 9px;">{{total_stock_akhir}} {{stock_awal.nama_satuan}}</th>
        </tr>
      </tbody>
    </table>
  </body>

</html>